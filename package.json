{
  "name": "vue-color",
  "version": "2.6.0",
  "description": "Color of Vue Components",
  "keywords": [
    "color",
    "vuejs",
    "color-picker"
  ],
  "main": "dist/vue-color.min.js",
  "scripts": {
    "example": "node build/build/example.js",
    "gh": "npm run example && node build/deploy",
    "lint": "eslint --ext .js,.vue src",
    "test": "jest",
    "release": "npm run lint && npm run test && webpack --progress --hide-modules --config  ./build/webpack.release.js && webpack --progress --hide-modules --config  ./build/webpack.release.min.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaokaike/vue-color"
  },
  "author": "xiaokai <kexiaokai@gmail.com>",
  "dependencies": {
    "clamp": "^1.0.1",
    "lodash.throttle": "^4.0.0",
    "material-colors": "^1.2.6",
    "tinycolor2": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/vue": "^5.3.14",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-preset-vue": "^2.0.2",
    "chalk": "^2.4.2",
    "css-loader": "^0.28.11",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^21.27.2",
    "eslint-plugin-vue": "^5.2.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gh-pages": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.18.0",
    "jest": "^23.6.0",
    "jest-canvas-mock": "^1.1.0",
    "jest-serializer-vue": "^2.0.2",
    "ora": "^1.4.0",
    "random-int": "^1.0.0",
    "shelljs": "^0.8.3",
    "url-loader": "^1.1.2",
    "vue": "^2.6.11",
    "vue-jest": "^2.6.0",
    "vue-loader": "^13.7.3",
    "vue-style-loader": "^3.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.0",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ]
  },
  "license": "MIT"
}
