{
  "name": "vue-color",
  "version": "2.7.1",
  "description": "Color of Vue Components",
  "keywords": [
    "color",
    "vuejs",
    "color-picker"
  ],
  "main": "dist/vue-color.min.js",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "start:example": "webpack-dev-server --config ./example/build/webpack.config.dev.js --open",
    "build:storybook": "build-storybook",
    "build": "webpack --config ./build/webpack.config.js",
    "build:example": "webpack --config ./example/build/webpack.config.js",
    "gh": "npm run build:example && node build/deploy",
    "lint": "eslint --ext .ts,.vue src",
    "test": "jest",
    "release": "npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaokaike/vue-color"
  },
  "author": "xiaokai <kexiaokai@gmail.com>",
  "dependencies": {
    "@types/tinycolor2": "^1.4.2",
    "clamp": "^1.0.1",
    "lodash.throttle": "^4.0.0",
    "material-colors": "^1.2.6",
    "tinycolor2": "^1.1.2",
    "vue-property-decorator": "^8.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/preset-typescript": "^2.1.0",
    "@storybook/vue": "^5.3.14",
    "@typescript-eslint/parser": "^2.22.0",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-vue": "^6.2.1",
    "fork-ts-checker-webpack-plugin": "^4.0.5",
    "gh-pages": "^1.2.0",
    "html-webpack-plugin": "^4.0.0-beta.9",
    "jest": "^23.6.0",
    "jest-canvas-mock": "^1.1.0",
    "jest-serializer-vue": "^2.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "random-int": "^1.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "vue": "^2.6.11",
    "vue-jest": "^2.6.0",
    "vue-loader": "^15.9.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ]
  },
  "license": "MIT"
}
